{
  "name": "react_webpack-starter",
  "version": "0.0.1",
  "description": "A basic react webpack boilerplate",
  "main": "./src/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.production --progress --profile --colors",
    "dev": "webpack-dev-server --config webpack.config.development --progress --profile --colors --hot",
    "ghpage": "npm run build && git-directory-deploy --directory public --branch gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stamen/react_webpack_starter"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "author": "Sean Connelley <connelley@stamen.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stamen/react_webpack_starter/issues"
  },
  "homepage": "",
  "dependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.21.0",
    "eslint": "^1.7.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-react": "^3.6.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "git-directory-deploy": "^1.3.0",
    "json-loader": "^0.5.3",
    "node-sass": "^3.3.3",
    "react-hot-loader": "^1.3.0",
    "resolve-url-loader": "^1.3.0",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  }
}
